<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="oopl" nsURI="http://www.zeligsoft.com/xtumlrt/oopl" nsPrefix="oopl">
  <eClassifiers xsi:type="ecore:EClass" name="OOPLClass" abstract="true">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Representation of XTUML class ({@link org.eclipse.papyrusrt.xtumlrt.xtuml.model.XTClass}) for OOP languages."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="xtClass" lowerBound="1"
        eType="ecore:EClass ../../org.eclipse.papyrusrt.xtumlrt.xtuml.model/model/xtuml.ecore#//XTClass"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="referenceStorage" upperBound="-1"
        eType="#//OOPLClassReferenceStorage"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OOPLType" abstract="true">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Representation of common type ({@link org.eclipse.papyrusrt.xtumlrt.common.model.Type}) for OOP languages."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="commonType" lowerBound="1"
        eType="ecore:EClass ../../org.eclipse.papyrusrt.xtumlrt.common.model/model/common.ecore#//Type"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OOPLClassReference" abstract="true"
      eSuperTypes="#//OOPLType">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Reference type in OOP languages to store reference to the OOP representation of a model class ({@link org.eclipse.papyrusrt.xtumlrt.oopl.model.OOPClass})."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="class" lowerBound="1" eType="#//OOPLClass"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OOPLClassReferenceCollection" abstract="true"
      eSuperTypes="#//OOPLType">
    <eStructuralFeatures xsi:type="ecore:EReference" name="class" lowerBound="1" eType="#//OOPLClass"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OOPLClassRefSimpleCollection" abstract="true"
      eSuperTypes="#//OOPLClassReferenceCollection">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="kind" eType="#//SimpleCollectionKind"
        defaultValueLiteral="SIMPLY_LINKED_LIST"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OOPLClassRefAssocCollection" abstract="true"
      eSuperTypes="#//OOPLClassReferenceCollection">
    <eStructuralFeatures xsi:type="ecore:EReference" name="key" lowerBound="1" eType="ecore:EClass ../../org.eclipse.papyrusrt.xtumlrt.common.model/model/common.ecore#//Attribute"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="kind" ordered="false" unique="false"
        eType="#//AssociativeCollectionKind" defaultValueLiteral="UNORDERED_SET"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="SimpleCollectionKind">
    <eLiterals name="INDEXED_ARRAY"/>
    <eLiterals name="SIMPLY_LINKED_LIST" value="1"/>
    <eLiterals name="DOUBLY_LINKED_LIST" value="2"/>
    <eLiterals name="DOUBLE_ENDED_QUEUE" value="3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="AssociativeCollectionKind">
    <eLiterals name="UNORDERED_SET"/>
    <eLiterals name="ORDERED_SET" value="1"/>
    <eLiterals name="UNORDERED_MULTISET" value="2"/>
    <eLiterals name="ORDERED_MULTISET" value="3"/>
    <eLiterals name="UNORDERED_MAP" value="4"/>
    <eLiterals name="ORDERED_MAP" value="5"/>
    <eLiterals name="UNORDERED_MULTIMAP" value="6"/>
    <eLiterals name="ORDERED_MULTIMAP" value="7"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OOPLClassReferenceStorage" abstract="true">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Storage for references of a class ({@link org.eclipse.papyrusrt.xtumlrt.oopl.model.OOPClass}) for queries that are made directly on the class or for a relation ({@link org.eclipse.papyrusrt.xtumlrt.oopl.model.OOPRelation}) if queries are made across the relation. Storage may be a single reference (for signleton classes and x-to-one relations) or a collection of references (for all other classes and x-to-many relations). Multiple storages for the same class or relation are possible if different queries require different access to the references (e.g. maps with different keys)."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" lowerBound="1" eType="#//OOPLType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OOPLRelation" abstract="true">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Representation of XTUML relation ({@link org.eclipse.papyrusrt.xtumlrt.xtuml.model.XTRelation}) for OOP languages."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="xtRelation" lowerBound="1"
        eType="ecore:EClass ../../org.eclipse.papyrusrt.xtumlrt.xtuml.model/model/xtuml.ecore#//XTRelation"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="referenceStorage" upperBound="-1"
        eType="#//OOPLClassReferenceStorage"/>
  </eClassifiers>
</ecore:EPackage>

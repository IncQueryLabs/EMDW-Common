<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.papyrus.extensionpoints.editors.DirectEditor">
      <DirectEditor
            language="rALF"
            objectToEdit="org.eclipse.uml2.uml.OpaqueExpression">
         <Priority
               name="High">
         </Priority>
         <simpleeditor
               editorConfiguration="com.incquerylabs.uml.ralf.ui.ReducedAlfDirectEditorConfiguration">
         </simpleeditor>
      </DirectEditor>
      <DirectEditor
            language="rALF"
            objectToEdit="org.eclipse.uml2.uml.OpaqueBehavior">
         <Priority
               name="High">
         </Priority>
         <simpleeditor
               editorConfiguration="com.incquerylabs.uml.ralf.ui.ReducedAlfDirectEditorConfiguration">
         </simpleeditor>
      </DirectEditor>
   </extension>
   <extension
         point="org.eclipse.papyrus.uml.properties.languageEditor">
      <editor
            editor="org.eclipse.papyrus.uml.properties.xtext.XtextLanguageEditor"
            language="rALF">
      </editor>
   </extension>

</plugin>
